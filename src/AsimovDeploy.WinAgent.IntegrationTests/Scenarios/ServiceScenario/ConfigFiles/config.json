{
  "WebPort": %AGENT_PORT%,
  "DataFolder": "%DATA_FOLDER%",
  "HeartbeatIntervalSeconds": 10,
  "NodeFrontUrl": "%NODE_FRONT_URL%",
  "Environment": "dev",
  "ConfigVersion": 1,

  "PackageSources": {
    "Test": {
      "Type": "FileSystem",
      "Uri": "%PACKAGES_URI%"
    }
  },

  "Units": [
    {
      "Name": "Asimov.Roundhouse.Example",
      "Type": "WindowsService",
      "TargetPath": "%DATA_FOLDER%\\WindowsServiceTarget",

      "PackageInfo": { "Source": "Test" },

      "Service": {
        "TargetPath": "%DATA_FOLDER%\\WindowsServiceTarget",
        "Install": ".\\Asimov.Roundhouse.Example.exe install",
        "Uninstall": ".\\Asimov.Roundhouse.Example.exe uninstall"
      }

    }


  ]
}